{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": "2018",
      "sourceType": "module"
    },
    "rules": {
        "no-plusplus": "off",
        "no-console": "off",
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ]
  }
}